{
  "$schema": "http://json-schema.org/draft/2020-12/schema",
  "title": "ClaimResponse",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "submission_id": {
      "type": "string"
    },
    "submission_period": {
      "type": "string"
    },
    "schedule_reference": {
      "type": "string",
      "validationErrorMessages": [
        {
          "key": "LEGAL HELP",
          "value": "Schedule Reference must be a maximum of 20 characters and contain only letters, numbers, forward slashes, periods, and hyphens"
        }
      ]
    },
    "line_number": {
      "type": "integer",
      "minimum": 1
    },
    "case_reference_number": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9/.\\-\\s]+$",
      "maxLength": 30,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Case Reference Number must contain only letters, numbers, forward slashes, periods, hyphens, and spaces, and be a maximum of 30 characters"
        }
      ]
    },
    "unique_file_number": {
      "type": "string",
      "pattern": "^[0-9]{6}/[0-9]{3}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Unique File Number (UFN) must be in the format DDMMYY/NNN with a date in the past"
        }
      ]
    },
    "case_start_date": {
      "type": "string",
      "pattern": "^(19\\d{2}|20\\d{2}|2100)-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Case Start Date must be a valid date in the format DD/MM/YYYY"
        }
      ]
    },
    "case_concluded_date": {
      "type": "string",
      "pattern": "^(19\\d{2}|20\\d{2}|2100)-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Case Concluded Date must be a valid date in the format DD/MM/YYYY"
        }
      ]
    },
    "matter_type_code": {
      "type": "string",
      "validationErrorMessages": [
        {
          "key": "LEGAL HELP",
          "value": "Each Matter Type Code 1 and 2 must be 4 characters"
        },
        {
          "key": "MEDIATION",
          "value": "Each Matter Type Code 1 and 2 must be 4 uppercase characters"
        }
      ]

    },
    "crime_matter_type_code": {
      "type": "string",
      "pattern": "^[0-9]{1,2}$",
      "validationErrorMessages": [
        {
          "key": "CRIME LOWER",
          "value": "Crime Lower Matter Type Code must be exactly 2 digits"
        }
      ]
    },
    "fee_scheme_code": {
      "type": "string"
    },
    "fee_code": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$",
      "maxLength": 10,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Fee Code must contain only letters and numbers, and be a maximum of 10 characters"
        }
      ]
    },
    "procurement_area_code": {
      "type": "string",
      "pattern": "^[A-Z]{2}[0-9]{5}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Procurement Area Code must be 2 uppercase letters followed by 5 digits"
        }
      ]
    },
    "access_point_code": {
      "type": "string",
      "pattern": "^AP[0-9]{5}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Access Point Code must be in the format AP##### (AP followed by 5 digits)"
        }
      ]
    },
    "delivery_location": {
      "type": "string",
      "pattern": "^[A-Z]{2}[0-9]{5}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Delivery Location must be 2 uppercase letters followed by 5 digits"
        }
      ]
    },
    "representation_order_date": {
      "type": "string",
      "pattern": "^(19\\d{2}|20\\d{2}|2100)-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Representation Order Date must be a valid date in the format DD/MM/YYYY"
        }
      ]
    },
    "suspects_defendants_count": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Suspects Defendants Count must be between 0 and 99"
        }
      ]
    },
    "police_station_court_attendances_count": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Police Station Court Attendances Count must be between 0 and 99"
        }
      ]
    },
    "police_station_court_prison_id": {
      "type": "string",
      "pattern": "^(?=.*[A-Za-z])[A-Za-z0-9]{1,6}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Police Station Court Prison ID must be 1â€“6 alphanumeric characters and contain at least one letter"
        }
      ]
    },
    "dscc_number": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{10}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "DSCC Number must be exactly 10 alphanumeric characters"
        }
      ]
    },
    "maat_id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{1,10}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "MAAT ID must be up to 10 alphanumeric characters"
        }
      ]
    },
    "prison_law_prior_approval_number": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{10}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Prison Law Prior Approval Number must be exactly 10 alphanumeric characters"
        }
      ]
    },
    "is_duty_solicitor": {
      "type": "boolean",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Duty Solicitor must be Y or N"
        }
      ]
    },
    "is_youth_court": {
      "type": "boolean",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Youth Court must be Y or N"
        }
      ]
    },
    "scheme_id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{4}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Scheme ID must be exactly 4 alphanumeric characters"
        }
      ]
    },
    "mediation_sessions_count": {
      "type": "integer",
      "minimum": 1,
      "maximum": 99,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Mediation Sessions Count must be between 1 and 99"
        }
      ]
    },
    "mediation_time_minutes": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99999,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Mediation Time Minutes must be between 0 and 99999"
        }
      ]
    },
    "outreach_location": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{3}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Outreach Location must be exactly 3 alphanumeric characters"
        }
      ]
    },
    "referral_source": {
      "type": "string",
      "pattern": "^(0[2-9]|1[0-1])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Referral Source must be a valid 2-digit code (02-11)"
        }
      ]
    },
    "total_value": {
      "type": "number"
    },
    "total_warnings": {
      "type": "number"
    },
    "client_forename": {
      "type": "string",
      "pattern": "^[\\p{L}\\p{N}\\p{Zs}\\-''&]{1,30}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client Forename must contain only letters, numbers, spaces, hyphens, apostrophes, ampersands, and be a maximum of 30 characters"
        }
      ]
    },
    "client_surname": {
      "type": "string",
      "pattern": "^[\\p{L}\\p{N}\\p{Zs}\\-''&]{1,30}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client Surname must contain only letters, numbers, spaces, hyphens, apostrophes, ampersands, and be a maximum of 30 characters"
        }
      ]
    },
    "client_date_of_birth": {
      "type": "string",
      "pattern": "^([0-9]{4})-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client Date of Birth must be a valid date in the format DD/MM/YYYY"
        }
      ]
    },
    "unique_client_number": {
      "type": "string",
      "pattern": "^(0[1-9]|[12][0-9]|3[01])(0[1-9]|1[0-2])([0-9]{4})/[\\p{L}0-9 \\-''&]/[\\p{L}0-9 \\-''&]{1,4}$",
      "maxLength": 15,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Unique Client Number must be in the format DDMMYYYY/X/ZZZZ with valid date, and be a maximum of 15 characters"
        }
      ]
    },
    "client_postcode": {
      "type": "string",
      "pattern": "^NFA|[A-Z]{1,2}[0-9][0-9A-Z]?\\s?[0-9][A-Z]{2}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client Postcode must be a valid UK postcode or NFA"
        }
      ]
    },
    "gender_code": {
      "type": "string",
      "pattern": "^([MFU])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Gender code must be valid"
        }
      ]
    },
    "ethnicity_code": {
      "type": "string",
      "pattern": "^(0[0-9]|1[0-6]|99)$",
      "minLength": 2,
      "maxLength": 2,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Ethnicity Code must be valid"
        }
      ]
    },
    "disability_code": {
      "type": "string",
      "pattern": "^(NCD|MOB|DEA|HEA|VIS|BLI|MHC|LDD|COG|ILL|OTH|UKN|PHY|SEN)$",
      "minLength": 3,
      "maxLength": 3,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Disability Code must be valid"
        }
      ]
    },
    "is_legally_aided": {
      "type": "boolean",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Is Legally Aided must be Y or N"
        }
      ]
    },
    "client_type_code": {
      "type": "string",
      "pattern": "^([PCJ])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client Type Code must be valid"
        }
      ]
    },
    "home_office_client_number": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$",
      "maxLength": 16,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Home Office Client Number must contain only letters and numbers, and be a maximum of 16 characters"
        }
      ]
    },
    "cla_reference_number": {
      "type": "string",
      "pattern": "^[0-9]{1,7}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "CLA Reference Number must be between 1 and 7 digits"
        }
      ]
    },
    "cla_exemption_code": {
      "type": "string",
      "minLength": 4,
      "maxLength": 4,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "CLA Exemption Code must be exactly 4 characters"
        }
      ]
    },
    "client_2_forename": {
      "type": "string",
      "pattern": "^[\\p{L}\\p{N}\\p{Zs}\\-''&]{1,30}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Forename must contain only letters, numbers, spaces, hyphens, apostrophes, ampersands, and be a maximum of 30 characters"
        }
      ]
    },
    "client_2_surname": {
      "type": "string",
      "pattern": "^[\\p{L}\\p{N}\\p{Zs}\\-''&]{1,30}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Surname must contain only letters, numbers, spaces, hyphens, apostrophes, ampersands, and be a maximum of 30 characters"
        }
      ]
    },
    "client_2_date_of_birth": {
      "type": "string",
      "pattern": "^([0-9]{4})-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Date of Birth must be a valid date in the format DD/MM/YYYY"
        }
      ]
    },
    "client_2_ucn": {
      "type": "string",
      "pattern": "^(0[1-9]|[12][0-9]|3[01])(0[1-9]|1[0-2])([0-9]{4})/[\\p{L}0-9 \\-''&]/[\\p{L}0-9 \\-''&]{1,4}$",
      "maxLength": 15,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Unique Client Number must be in the format DDMMYYYY/X/ZZZZ with valid date, and be a maximum of 15 characters"
        }
      ]
    },
    "client_2_postcode": {
      "type": "string",
      "pattern": "^NFA|[A-Z]{1,2}[0-9][0-9A-Z]?\\s?[0-9][A-Z]{2}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Postcode must be a valid UK postcode or NFA"
        }
      ]
    },
    "client_2_gender_code": {
      "type": "string",
      "pattern": "^([MFU])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Gender code must be valid"
        }
      ]
    },
    "client_2_ethnicity_code": {
      "type": "string",
      "pattern": "^(0[0-9]|1[0-6]|99)$",
      "minLength": 2,
      "maxLength": 2,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Ethnicity Code must be valid"
        }
      ]
    },
    "client_2_disability_code": {
      "type": "string",
      "pattern": "^(NCD|MOB|DEA|HEA|VIS|BLI|MHC|LDD|COG|ILL|OTH|UKN|PHY|SEN)$",
      "minLength": 3,
      "maxLength": 3,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Disability Code must be valid"
        }
      ]
    },
    "client_2_is_legally_aided": {
      "type": "boolean",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Is Legally Aided must be Y or N"
        }
      ]
    },
    "case_id": {
      "type": "string",
      "pattern": "^[0-9]{3}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Case ID must be exactly 3 digits"
        }
      ]
    },
    "unique_case_id": {
      "type": "string",
      "pattern": "^\\s*\\S.*$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Unique Case ID must contain at least one non-whitespace character"
        }
      ]
    },
    "case_stage_code": {
      "type": "string",
      "pattern": "^(FPL(0[1-9]|1[0-9]|20|21)|FPC0[1-3]|MHL(0[1-9]|10))$",
      "minLength": 5,
      "maxLength": 5,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Case Stage/Level Code must be valid"
        }
      ]
    },
    "stage_reached_code": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$",
      "minLength": 2,
      "maxLength": 4,
      "validationErrorMessages" : [
        {
          "key": "LEGAL HELP",
          "value": "Stage Reached Code must be exactly 2 alphanumeric characters for Legal Help claims"
        },
        {
          "key": "CRIME LOWER",
          "value": "Stage Reached Code must be exactly 4 uppercase letters for Crime Lower claims"
        }
      ]
    },
    "standard_fee_category_code": {
      "type": "string",
      "pattern": "^(1A|2A|1B|2B|1A-HSF|1B-HSF|1A-LSF|1B-LSF|1EW|1SO|2|3|ULF|UHF|CLF|CHF|Sending Hearing Fixed Fee)$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Standard Fee Category Code must be valid"
        }
      ]
    },
    "outcome_code": {
      "type": "string",
      "validationErrorMessages": [
        {
          "key": "LEGAL HELP",
          "value": "Outcome Code must be exactly 2 characters and contain only letters, numbers, and hyphens"
        },
        {
          "key": "CRIME LOWER",
          "value": "Outcome Code must be a valid crime lower outcome code or left blank"
        },
        {
          "key": "MEDIATION",
          "value": "Outcome Code must be a valid mediation outcome code or left blank"
        }
      ]
    },
    "designated_accredited_representative_code": {
      "type": "string",
      "pattern": "^[1-5]$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Designated Accredited Representative Code must be valid"
        }
      ]
    },
    "is_postal_application_accepted": {
      "type": "boolean",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Postal Application Accepted must be Y or N"
        }
      ]
    },
    "is_client_2_postal_application_accepted": {
      "type": "boolean",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Postal Application Accepted must be Y or N"
        }
      ]
    },
    "mental_health_tribunal_reference": {
      "type": "string",
      "pattern": "^([A-Z]{2}/[0-9]{4}/[0-9]{4}|[A-Z]{2}[0-9]{6})$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Mental Health Tribunal Reference must be in format XX/YYYY/YYYY or XXYYYZZZ"
        }
      ]
    },
    "is_nrm_advice": {
      "type": "boolean",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "NRM Advice must be Y or N"
        }
      ]
    },
    "follow_on_work": {
      "type": "string",
      "minLength": 1,
      "maxLength": 1,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Follow On Work must be a single character"
        }
      ]
    },
    "transfer_date": {
      "type": "string",
      "pattern": "^(19\\d{2}|20\\d{2}|2100)-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Transfer Date must be a valid date in the format DD/MM/YYYY"
        }
      ]
    },
    "exemption_criteria_satisfied": {
      "type": "string",
      "pattern": "^[A-Z]{2}[0-9]{3}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Exemption Criteria Satisfied must be 2 uppercase letters followed by 3 digits"
        }
      ]
    },
    "exceptional_case_funding_reference": {
      "type": "string",
      "pattern": "^[0-9]{7}[A-Z]{2}$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Exceptional Case Funding Reference must be 7 digits followed by 2 uppercase letters"
        }
      ]
    },
    "is_legacy_case": {
      "type": "boolean",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Legacy Case must be Y or N"
        }
      ]
    },
    "advice_time": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99999,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Advice Time must be in minutes"
        }
      ]
    },
    "travel_time": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99999,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Travel Time must be in minutes"
        }
      ]
    },
    "waiting_time": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99999,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Waiting Time must be in minutes"
        }
      ]
    },
    "net_profit_costs_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 999999999.99,
      "multipleOf": 0.01,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Net Profit Costs Amount must be a valid monetary value"
        }
      ]
    },
    "net_disbursement_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 999999999.99,
      "multipleOf": 0.01,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Net Disbursement Amount must be a valid monetary value"
        }
      ]
    },
    "net_counsel_costs_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 99999.99,
      "multipleOf": 0.01,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Net Counsel Costs Amount must be a valid monetary value"
        }
      ]
    },
    "disbursements_vat_amount": {
      "type": "number",
      "minimum": 0.0,
      "multipleOf": 0.01,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Disbursements VAT Amount must be a valid monetary value"
        }
      ]
    },
    "travel_waiting_costs_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 9999.99,
      "multipleOf": 0.01,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Travel Waiting Costs Amount must be a valid monetary value"
        }
      ]
    },
    "net_waiting_costs_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 999999.99,
      "multipleOf": 0.01,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Net Waiting Costs Amount must be a valid monetary value"
        }
      ]
    },
    "is_vat_applicable": {
      "type": "boolean",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "VAT Applicable must be Y or N"
        }
      ]
    },
    "is_tolerance_applicable": {
      "type": "boolean",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Is Tolerance Applicable must be true or false"
        }
      ]
    },
    "prior_authority_reference": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$",
      "minLength": 7,
      "maxLength": 7,
