{
  "$schema": "http://json-schema.org/draft/2020-12/schema",
  "title": "ClaimResponse",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "submission_id": {
      "type": "string"
    },
    "submission_period": {
      "type": "string"
    },
    "schedule_reference": {
      "type": "string",
      "validationErrorMessages": [
        {
          "key": "LEGAL HELP",
          "value": "Schedule Reference must be a maximum of 20 characters and contain only letters, numbers, forward slashes, periods, and hyphens"
        }
      ]
    },
    "line_number": {
      "type": "integer",
      "minimum": 1
    },
    "case_reference_number": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9/.\\-\\s]+$",
      "maxLength": 30
    },
    "unique_file_number": {
      "type": "string",
      "pattern": "^[0-9]{6}/[0-9]{3}$"
    },
    "case_start_date": {
      "type": "string",
        "pattern": "^(19\\d{2}|20\\d{2}|2100)-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])$"
    },
    "case_concluded_date": {
      "type": "string",
        "pattern": "^(19\\d{2}|20\\d{2}|2100)-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])$"
    },
    "matter_type_code": {
      "type": "string",
      "validationErrorMessages": [
        {
          "key": "LEGAL HELP",
          "value": "Each Matter Type Code 1 and 2 must be 4 characters"
        },
        {
          "key": "MEDIATION",
          "value": "Each Matter Type Code 1 and 2 must be 4 uppercase characters"
        }
      ]

    },
    "crime_matter_type_code": {
      "type": "string",
      "pattern": "^[0-9]{1,2}$"
    },
    "fee_scheme_code": {
      "type": "string"
    },
    "fee_code": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$",
      "maxLength": 10
    },
    "procurement_area_code": {
      "type": "string",
      "pattern": "^[A-Z]{2}[0-9]{5}$"
    },
    "access_point_code": {
      "type": "string",
      "pattern": "^AP[0-9]{5}$"
    },
    "delivery_location": {
      "type": "string",
      "pattern": "^[A-Z]{2}[0-9]{5}$"
    },
    "representation_order_date": {
      "type": "string",
        "pattern": "^(19\\d{2}|20\\d{2}|2100)-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])$"
    },
    "suspects_defendants_count": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99
    },
    "police_station_court_attendances_count": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99
    },
    "police_station_court_prison_id": {
      "type": "string",
      "pattern": "^(?=.*[A-Za-z])[A-Za-z0-9]{1,6}$"
    },
    "dscc_number": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{10}$"
    },
    "maat_id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{10}$"
    },
    "prison_law_prior_approval_number": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{10}$"
    },
    "is_duty_solicitor": {
      "type": "boolean"
    },
    "is_youth_court": {
      "type": "boolean"
    },
    "scheme_id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{4}$"
    },
    "mediation_sessions_count": {
      "type": "integer",
      "minimum": 1,
      "maximum": 99
    },
    "mediation_time_minutes": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99999
    },
    "outreach_location": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{3}$"
    },
    "referral_source": {
      "type": "string",
      "pattern": "^(0[2-9]|1[0-1])$"
    },
    "total_value": {
      "type": "number"
    },
    "client_forename": {
      "type": "string",
      "pattern": "^[\\p{L}\\p{N}\\p{Zs}\\-’'&]{1,30}$"
    },
    "client_surname": {
      "type": "string",
      "pattern": "^[\\p{L}\\p{N}\\p{Zs}\\-’'&]{1,30}$"
    },
    "client_date_of_birth": {
      "type": "string",
        "pattern": "^(19\\d{2}|20\\d{2}|2100)-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])$"
    },
    "unique_client_number": {
      "type": "string",
      "pattern": "^(0[1-9]|[12][0-9]|3[01])(0[1-9]|1[0-2])(19[0-9]{2}|20[0-9]{2})/[\\p{L}0-9 \\-’'&]/[\\p{L}0-9 \\-’'&]{1,4}$",
      "maxLength": 15
    },
    "client_postcode": {
      "type": "string",
      "pattern": "^NFA|[A-Z]{1,2}[0-9][0-9A-Z]?\\s?[0-9][A-Z]{2}$"
    },
    "gender_code": {
      "type": "string",
      "pattern": "^([MFU])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Gender code must be valid"
        }
      ]
    },
    "ethnicity_code": {
      "type": "string",
      "pattern": "^(0[0-9]|1[0-6]|99)$",
      "minLength": 2,
      "maxLength": 2,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Ethnicity Code must be valid"
        }
      ]
    },
    "disability_code": {
      "type": "string",
      "pattern": "^(NCD|MOB|DEA|HEA|VIS|BLI|MHC|LDD|COG|ILL|OTH|UKN|PHY|SEN)$",
      "minLength": 3,
      "maxLength": 3
    },
    "is_legally_aided": {
      "type": "boolean"
    },
    "client_type_code": {
      "type": "string",
      "pattern": "^([PCJ])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client Type Code must be valid"
        }
      ]
    },
    "home_office_client_number": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$",
      "maxLength": 16
    },
    "cla_reference_number": {
      "type": "string",
      "pattern": "^[0-9]{1,7}$"
    },
    "cla_exemption_code": {
      "type": "string",
      "minLength": 4,
      "maxLength": 4
    },
    "client_2_forename": {
      "type": "string",
      "pattern": "^[\\p{L}\\p{N}\\p{Zs}\\-’'&]{1,30}$"
    },
    "client_2_surname": {
      "type": "string",
      "pattern": "^[\\p{L}\\p{N}\\p{Zs}\\-’'&]{1,30}$"
    },
    "client_2_date_of_birth": {
      "type": "string",
        "pattern": "^(19\\d{2}|20\\d{2}|2100)-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])$"
    },
    "client_2_ucn": {
      "type": "string",
      "pattern": "^(0[1-9]|[12][0-9]|3[01])(0[1-9]|1[0-2])(19[0-9]{2}|20[0-9]{2})/[\\p{L}0-9 \\-’'&]/[\\p{L}0-9 \\-’'&]{1,4}$",
      "maxLength": 15
    },
    "client_2_postcode": {
      "type": "string",
      "pattern": "^NFA|[A-Z]{1,2}[0-9][0-9A-Z]?\\s?[0-9][A-Z]{2}$"
    },
    "client_2_gender_code": {
      "type": "string",
      "pattern": "^([MFU])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Gender code must be valid"
        }
      ]
    },
    "client_2_ethnicity_code": {
      "type": "string",
      "pattern": "^(0[0-9]|1[0-6]|99)$",
      "minLength": 2,
      "maxLength": 2,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Ethnicity Code must be valid"
        }
      ]
    },
    "client_2_disability_code": {
      "type": "string",
      "pattern": "^(NCD|MOB|DEA|HEA|VIS|BLI|MHC|LDD|COG|ILL|OTH|UKN|PHY|SEN)$",
      "minLength": 3,
      "maxLength": 3,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Client 2 Disability Code must be valid"
        }
      ]
    },
    "client_2_is_legally_aided": {
      "type": "boolean"
    },
    "case_id": {
      "type": "string",
      "pattern": "^[0-9]{3}$"
},
    "unique_case_id": {
      "type": "string",
      "pattern": "^\\s*\\S.*$"
    },
    "case_stage_code": {
      "type": "string",
      "pattern": "^(FPL(0[1-9]|1[0-9]|20|21)|FPC0[1-3]|MHL(0[1-9]|10))$",
      "minLength": 5,
      "maxLength": 5,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Case Stage/Level Code must be valid"
        }
      ]
    },
    "stage_reached_code": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$",
      "minLength": 2,
      "maxLength": 4,
      "validationErrorMessages" : [
        {
          "key": "LEGAL HELP",
          "value": "StageReached Code must be exactly 2 alphanumeric characters for Civil claims"
        },
        {
          "key": "CRIME",
          "value": "StageReached Code must be exactly 4 uppercase letters for Crime claims"
        }
      ]
    },
    "standard_fee_category_code": {
      "type": "string",
      "pattern": "^(1A|2A|1B|2B|1A-HSF|1B-HSF|1A-LSF|1B-LSF|1EW|1SO|2|3|ULF|UHF|CLF|CHF|Sending Hearing Fixed Fee)$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Standard Fee Category Code must be valid"
        }
      ]
    },
    "outcome_code": {
      "type": "string",
      "validationErrorMessages": [
        {
          "key": "LEGAL HELP",
          "value": "Outcome Code must be exactly 2 characters and contain only letters, numbers, and hyphens"
        },
        {
          "key": "CRIME LOWER",
          "value": "Outcome Code must be a valid crime outcome code or left blank"
        },
        {
          "key": "MEDIATION",
          "value": "Outcome Code must a valid mediation outcome code or left blank"
        }
      ]
    },
    "designated_accredited_representative_code": {
      "type": "string",
      "pattern": "^[1-5]$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Designated Accredited Representative Code must be valid"
        }
      ]
    },
    "is_postal_application_accepted": {
      "type": "boolean"
    },
    "is_client_2_postal_application_accepted": {
      "type": "boolean"
    },
    "mental_health_tribunal_reference": {
      "type": "string",
      "pattern": "^([A-Z]{2}/[0-9]{4}/[0-9]{4}|[A-Z]{2}[0-9]{6})$"
    },
    "is_nrm_advice": {
      "type": "boolean"
    },
    "follow_on_work": {
      "type": "string",
      "minLength": 1,
      "maxLength": 1
    },
    "transfer_date": {
      "type": "string",
        "pattern": "^(19\\d{2}|20\\d{2}|2100)-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])$"
    },
    "exemption_criteria_satisfied": {
      "type": "string",
      "pattern": "^[A-Z]{2}[0-9]{3}$"
    },
    "exceptional_case_funding_reference": {
      "type": "string",
      "pattern": "^[0-9]{7}[A-Z]{2}$"
    },
    "is_legacy_case": {
      "type": "boolean"
    },
    "advice_time": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99999
    },
    "travel_time": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99999
    },
    "waiting_time": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99999
    },
    "net_profit_costs_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 999999999.99,
      "multipleOf": 0.01
    },
    "net_disbursement_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 999999999.99,
      "multipleOf": 0.01
    },
    "net_counsel_costs_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 99999.99,
      "multipleOf": 0.01
    },
    "disbursements_vat_amount": {
      "type": "number",
      "minimum": 0.0,
      "multipleOf": 0.01
    },
    "travel_waiting_costs_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 9999.99,
      "multipleOf": 0.01
    },
    "net_waiting_costs_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 999999.99,
      "multipleOf": 0.01
    },
    "is_vat_applicable": {
      "type": "boolean"
    },
    "is_tolerance_applicable": {
      "type": "boolean"
    },
    "prior_authority_reference": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$",
      "minLength": 7,
      "maxLength": 7
    },
    "is_london_rate": {
      "type": "boolean"
    },
    "adjourned_hearing_fee_amount": {
      "type": "integer",
      "minimum": 0,
      "maximum": 9
    },
    "is_additional_travel_payment": {
      "type": "boolean"
    },
    "costs_damages_recovered_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 99999.99,
      "multipleOf": 0.01
    },
    "meetings_attended_code": {
      "type": "string",
      "pattern": "^MTGA(0[1-9]|1[0-9]|2[0-4])$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Meetings Attended Code must be valid"
        }
      ]
    },
    "detention_travel_waiting_costs_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 99999999.99,
      "multipleOf": 0.01
    },
    "jr_form_filling_amount": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 9999.99,
      "multipleOf": 0.01
    },
    "is_eligible_client": {
      "type": "boolean"
    },
    "court_location_code": {
      "type": "string"
    },
    "advice_type_code": {
      "type": "string",
      "pattern": "^(FTF|REM)$",
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "Advice Type Code must be valid"
        }
      ]
    },
    "medical_reports_count": {
      "type": "integer",
      "minimum": 0,
      "maximum": 10
    },
    "is_irc_surgery": {
      "type": "boolean"
    },
    "surgery_date": {
      "type": "string",
      "maxLength": 30
    },
    "surgery_clients_count": {
      "type": "integer",
      "minimum": 1,
      "maximum": 20
    },
    "surgery_matters_count": {
      "type": "integer",
      "minimum": 1,
      "maximum": 20
    },
    "cmrh_oral_count": {
      "type": "integer",
      "minimum": 0,
      "maximum": 9
    },
    "cmrh_telephone_count": {
      "type": "integer",
      "minimum": 0,
      "maximum": 9
    },
    "ait_hearing_centre_code": {
      "type": "string",
      "pattern": "^(0[1-9]|1[0-6])$",
      "minLength": 2,
      "maxLength": 2,
      "validationErrorMessages": [
        {
          "key": "ALL",
          "value": "AIT Hearing Centre Code must be valid"
        }
      ]
    },
    "is_substantive_hearing": {
      "type": "boolean"
    },
    "ho_interview": {
      "type": "integer",
      "minimum": 0,
      "maximum": 9
    },
    "local_authority_number": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$",
      "maxLength": 30
    },
    "created_by_user_id": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "status",
    "line_number",
    "net_disbursement_amount",
    "disbursements_vat_amount",
    "fee_code"
  ]
}